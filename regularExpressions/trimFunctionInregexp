  <script>
  //  1. Write a JavaScript program that works as a regular expression trim function (string). 

  const trimFirstLast = (string)=> {
  
  
    trimString = string.replace(/\s{2,}/g, ' ');
    
   
    return trimString;
  }

  const string = '                                                               Hi I am   anu ';

 console.log(string);
 console.log(trimFirstLast(string));
